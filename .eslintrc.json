{
  "extends": "airbnb-base",
  "env": {
    "browser": true
  },
  "globals": {
    "enigma": true,
    "d3": true
  },
  "rules": {
    "class-methods-use-this": 0,
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "no-restricted-properties": 0,
    "no-plusplus": [2, {
      "allowForLoopAfterthoughts": true
    }],
    "arrow-body-style": 1
  }
}
