  "bugs": {
    "url": "https://github.com/qlik-oss/core-experimenter/issues"
  },
  "homepage": "https://github.com/qlik-oss/core-experimenter#readme",
  "devDependencies": {
    "babel-core": "6.26.3",
    "concurrently": "3.6.1",
    "cross-env": "5.2.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "13.1.0",
    "eslint-plugin-import": "2.17.3",
    "live-server": "1.2.1",
    "rollup": "1.16.2",
    "rollup-plugin-babel": "3.0.7",
    "rollup-plugin-commonjs": "10.0.0",
    "rollup-plugin-copy-glob": "0.3.0",
